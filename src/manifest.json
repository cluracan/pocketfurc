{
	"name": "PocketFurc",
	"description": "A rudimentary Furcadia client.",
	"version": "0.39",
	"author": "cluracan",
	"manifest_version": 2,
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"36": "icon36.png",
		"48": "icon48.png",
		"72": "icon72.png",
		"78": "icon78.png",
		"96": "icon96.png",
		"120": "icon120.png",
		"128": "icon128.png",
		"144": "icon144.png",
